{
    "name": "cases/som",
    "description": "Cases Composer install",
     "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/7"

        }
    },
    "require": {
        "cweagans/composer-patches": "^1.6",
