        "drupal/date": "^2.10",
        "drupal/devel": "^1.5",
        "drupal/domain": "^3.13",
        "drupal/domain_access_bridge": "^1.5",
        "drupal/domain_ctools": "^1.3",
        "drupal/drupal": "7.56",
        "drupal/ds": "^2.14",
        "drupal/elements": "^1.4",
        "drupal/elfinder": "^0.8.0",
        "drupal/empty_front_page": "^1.0",
        "drupal/entity": "1.8",
        "drupal/entityreference": "^1.5",
        "drupal/epub_export": "^1.0@beta",
        "drupal/features": "^2.10",
        "drupal/features_extra": "^1.0",
        "drupal/field_collection": "^1.0@beta",
        "drupal/field_collection_views": "^1.0@beta",
        "drupal/field_group": "^1.5",
        "drupal/field_permissions": "^1.0",
        "drupal/field_property": "^1.0@beta",
        "drupal/file_entity": "2.3",
