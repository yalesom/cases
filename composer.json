        "drupal/format_number": "1.x-dev",
        "drupal/fpa": "^2.6",
        "drupal/google_analytics": "^2.4",
        "drupal/google_tag": "^1.3",
        "drupal/html5_tools": "^1.3",
        "drupal/image_url_formatter": "^1.4",
        "drupal/imageapi_optimize": "^1.3",
        "drupal/imagefield_crop": "^2.0",
        "drupal/imagefield_focus": "^1.0",
        "drupal/imce": "^1.11",
        "drupal/imce_crop": "^1.1",
        "drupal/imce_filefield": "^1.1",
        "drupal/imce_mkdir": "^1.0",
        "drupal/imce_tools": "^1.2",
