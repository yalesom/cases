        "drupal/media": "^2.11",
        "drupal/memcache": "^1.6",
        "drupal/memcache_storage": "^1.4",
        "drupal/menu_block": "^2.7",
        "drupal/menu_position": "^1.2",
        "drupal/modernizr": "^3.10",
        "drupal/module_filter": "^2.1",
        "drupal/node_clone": "^1.0",
        "drupal/node_export": "^3.1",
        "drupal/nodeaccess": "^1.4",
        "drupal/nodequeue": "^2.1",
        "drupal/outline_designer": "^2.3",
        "drupal/pathauto": "^1.3",
        "drupal/querypath": "^2.1",
        "drupal/registry_rebuild": "^2.5",
        "drupal/relation": "^1.1",
        "drupal/rules": "^2.10",
        "drupal/semanticviews": "1.x-dev",
        "drupal/sheetnode": "^1.0@beta",
        "drupal/smartcrop": "^1.0@beta",
        "drupal/stage_file_proxy": "^1.7",
        "drupal/strongarm": "^2.0",
        "drupal/taxonomy_manager": "^1.0",
        "drupal/taxonomy_menu": "^1.5",
        "drupal/term_merge": "^1.4",
        "drupal/theme_editor": "2.x-dev",
        "drupal/token": "^1.7",
        "drupal/uuid": "^1.0",
        "drupal/varnish": "^1.5",
        "drupal/views": "^3.18",
        "drupal/views_bulk_operations": "3.4",
        "drupal/views_conditional": "^1.3",
        "drupal/views_content": "^1.12",
        "drupal/views_field_view": "^1.2",
        "drupal/views_php": "^1.0@alpha",
        "drupal/views_tree": "^2.0",
        "drupal/views_ui_basic": "^1.3",
        "drupal/workbench": "^1.2",
        "drupal/workbench_access": "^1.5",
        "drupal/workbench_media": "^2.1",
        "drupal/workbench_moderation": "^3.0",
        "drupal/wysiwyg": "^2.4"
    },
    "conflict": {
      "drupal/core": "8.*"
    },
    "scripts": {
        "post-update-cmd": [
          "rm -rf core-sites/sites",
          "./composer_post_install.sh",
          "rm -rf core-sites"
        ]
    },
    "config": {
      "vendor-dir": "vendor"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
      "custom-installer": {
        "drupal-module": "sites/all/modules/contrib/{$name}/",
        "drupal-theme": "sites/all/themes/contrib/{$name}/",
        "drupal-library": "sites/all/libraries/{$name}/",
        "drupal-drush": "sites/all/drush/{$name}/",
        "drupal-profile": "profiles/{$name}/",
        "drupal-core": "core-sites/"
      },
      "preserve-paths": [
        "sites/all/drush",
        "sites/default/settings.php",
        "sites/default/files",
        "sites/all/modules/contrib"
      ],
      "patches-file": "composer.patches.json",
      "enable-patching": true,
      "composer-exit-on-patch-failure": true
    }
}
